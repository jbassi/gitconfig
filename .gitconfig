[user]
	name =
	email =
[core]
	editor = /usr/bin/vim
[alias]
  st = status
  ci = commit
  cia = commit --amend
  br = branch
  co = checkout
  pr = pull --rebase
  ri = rebase -i HEAD~5
  lg = log -p
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ls = ls-files
  # Set upstream.
  su = branch -u
  # Show files ignored by git.
  ign = ls-files -o -i --exclude-standard
[push]
  default = simple
